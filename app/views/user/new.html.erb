<h1 class="form-group">Register</h1>
<%= form_for :user, url: "/user" do |f| %>
<div class="form-group">
   <%= f.label :name%>
   <%= f.text_field :name%>
</div>
<div class="form-group">
   <%= f.label :email%>
   <%= f.text_field :email%>
</div>
<div class="form-group">
   <%= f.label :password%>
   <%= f.password_field :password%>
</div>
<div class="form-group">
   <%= f.submit "Register", class: "btn btn-default" %>
</div>
<% end %>
